{
	"name": "@ontario-digital-service/ontario-design-system-component-library-react",
	"sideEffects": false,
	"version": "1.0.0",
	"private": true,
	"description": "React specific wrapper for ontario-design-system-component-library",
	"repository": {
		"type": "git",
		"url": "https://github.com/ionic-team/ionic.git"
	},
	"scripts": {
		"build": "npm run clean && npm run compile",
		"postbuild": "npm run copy:assets && npm run copy:icons",
		"clean": "rm -rf dist",
		"clean:full": "rm -rf node_modules && npm run clean",
		"compile": "npm run tsc",
		"tsc": "tsc -p .",
		"rollup": "rollup -c",
		"copy:icons": "copyfiles -E -f \"node_modules/@ontario-digital-service/ontario-design-system-component-library/dist/ontario-design-system-components/assets/icons/*\" dist/assets/icons",
		"copy:assets": "copyfiles -E -f \"node_modules/@ontario-digital-service/ontario-design-system-component-library/dist/ontario-design-system-components/assets/*\" dist/assets"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/"
	],
	"dependencies": {
		"@ontario-digital-service/ontario-design-system-component-library": "^1.0.0"
	},
	"devDependencies": {
		"@types/jest": "23.3.9",
		"@types/node": "^15.12.2",
		"@types/react": "16.7.6",
		"@types/react-dom": "16.0.9",
		"copyfiles": "^2.4.1",
		"jest": "^23.0.0",
		"jest-dom": "^3.0.2",
		"np": "^3.1.0",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"typescript": "^3.3.4000"
	},
	"peerDependencies": {
		"react": "^16.7.0",
		"react-dom": "^16.7.0"
	},
	"jest": {
		"preset": "ts-jest",
		"setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
		"testPathIgnorePatterns": [
			"node_modules",
			"dist"
		]
	},
	"authors": [
		{
			"name": "Scott Morris",
			"email": "Scott.Morris@ontario.ca"
		},
		{
			"name": "Elsa Ng",
			"email": "Elsa.Ng@ontario.ca"
		},
		{
			"name": "Erin Carrieres",
			"email": "Erin.Carrieres@ontario.ca"
		},
		{
			"name": "Meaghan Carrieres",
			"email": "Meaghan.Carrieres@ontario.ca"
		}
	]
}
