{
	"name": "react-app",
	"version": "2.0.0-beta.5",
	"private": true,
	"dependencies": {
		"@ontario-digital-service/ontario-design-system-component-library-react": "^2.0.0-beta.5",
		"@ontario-digital-service/ontario-design-system-global-styles": "^2.0.0-beta.5",
		"@ontario-digital-service/ontario-design-system-design-tokens": "^2.0.0-beta.5",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.15",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@types/react-syntax-highlighter": "^13.5.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^6.2.1",
		"react-scripts": "5.0.0",
		"react-syntax-highlighter": "^15.4.5",
		"sass": "^1.49.7",
		"typescript": "^4.5.4",
		"web-vitals": "^2.1.2"
	},
	"scripts": {
		"start": "npm run build && react-scripts start",
		"prebuild": "npm run copy:assets",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"clean": "rm -rf build",
		"clean:full": "rm -rf node_modules && npm run clean",
		"copy:images": "copyfiles -E -f \"node_modules/@ontario-digital-service/ontario-design-system-component-library-react/dist/assets/images/**\" public/assets",
		"copy:fonts": "copyfiles -E -u 6 \"node_modules/@ontario-digital-service/ontario-design-system-component-library-react/dist/assets/fonts/**/*\" public/assets/fonts",
		"copy:assets": "npm run copy:images && npm run copy:fonts"
	},
	"homepage": "./",
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"development": [
		"last 1 chrome version",
		"last 1 firefox version",
		"last 1 safari version"
	],
	"authors": [
