{
	"name": "@ontario-digital-service/ontario-design-system-component-library",
	"version": "2.0.0-alpha.3",
	"private": false,
	"description": "Stencil Component Starter",
	"main": "dist/index.cjs.js",
	"module": "dist/index.js",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs",
	"types": "dist/types/index.d.ts",
	"collection": "dist/collection/collection-manifest.json",
	"collection:main": "dist/collection/index.js",
	"unpkg": "dist/ontario-design-system-component-library/ontario-design-system-component-library.esm.js",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist/",
		"loader/",
		"src/",
		"www/",
		"LICENSE",
		"package.json",
		"readme.md"
	],
	"scripts": {
		"build": "stencil build --docs",
		"start": "stencil build --dev --watch --serve",
		"test": "stencil test --spec --e2e",
		"test:unit": "stencil test --spec",
		"test:e2e": "stencil test --e2e",
		"test.watch": "stencil test --spec --e2e --watchAll",
		"generate": "stencil generate",
		"clean": "rm -rf dist loader www",
		"clean:full": "rm -rf node_modules && npm run clean",
		"format": "prettier --write .",
		"generate:icons": "node src/components/ontario-icon/scripts/generate-icons.js",
		"clean:icons": "rm -rf src/components/ontario-icon/ontario-icon-*.tsx",
		"docs:assemble": "ts-node scripts/build-documentation-bundle.ts"
	},
	"dependencies": {
		"@stencil/core": "2.13.0",
		"validate-color": "^2.2.1",
		"@ontario-digital-service/ontario-design-system-global-styles": "2.0.0-alpha.3"
	},
	"devDependencies": {
		"@stencil/react-output-target": "0.0.9",
		"@stencil/sass": "^1.4.1",
		"@types/jest": "^26.0.24",
		"@types/puppeteer": "^5.4.3",
		"@types/uuid": "^8.3.3",
		"babel-jest": "^27.4.6",
		"fs-extra": "^10.0.0",
		"glob": "^7.2.0",
		"glob-promise": "^4.2.2",
		"jest": "^26.0.24",
		"jest-cli": "^26.6.3",
		"jest-junit": "^13.0.0",
		"jest-svg-transformer": "^1.0.0",
		"jszip": "^3.10.1",
		"prettier": "^2.5.1",
		"puppeteer": "^10.0.0",
		"stencil-inline-svg": "^1.1.0",
		"svgson": "^5.2.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.5.5",
		"validate-color": "^2.2.1"
	},
	"authors": [
		{
			"name": "Scott Morris",
			"email": "Scott.Morris@ontario.ca"
		},
		{
			"name": "Elsa Ng",
			"email": "Elsa.Ng@ontario.ca"
		},
		{
			"name": "Erin Carrieres",
			"email": "Erin.Carrieres@ontario.ca"
		},
		{
			"name": "Meaghan Carrieres",
			"email": "Meaghan.Carrieres@ontario.ca"
		},
		{
			"name": "Nakib Momin",
			"email": "nakib.momin@ontario.ca"
		}
	],
	"license": "Apache-2.0"
}
